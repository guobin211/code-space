    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "mongoose": "^6.4.4",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
