{
  "name": "code-space",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clear": "turbo run clear",
    "update:all": "turbo run upgrade",
    "coverage": "vitest run --coverage",
    "tsnode": "ts-node-esm",
    "test": "vitest run",
    "prepare": "husky install",
    "turbo": "turbo",
    "rome": "rome"
  },
  "type": "module",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "@swc/wasm": "^1.3.11",
    "@types/node": "^18",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "husky": "^8",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "turbo": "latest",
    "typescript": "^4",
    "vite": "^3",
    "vitest": "^0.24"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@7.12.2"
}
