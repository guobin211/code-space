{
  "name": "code-space",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "vite build",
    "clear:all": "pnpm run --filter '*' clear && rimraf node_modules",
    "dev": "vite dev",
    "format": "node scripts/format.mjs",
    "lint": "node scripts/eslint.mjs",
    "prepare": "husky install",
    "test": "vitest run",
    "test:all": "pnpm run --filter '*' test",
    "upgrade:all": "pnpm run --filter '*' upgrade"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "sass": "^1.62.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-top-level-await": "^1.3.0",
    "vite-plugin-wasm": "^3.2.2",
    "vitest": "^0.25.8"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.8.0",
    "normalize.css": "^8.0.1",
    "postcss-preset-env": "^8.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0"
  },
  "engines": {
    "node": ">=18"
  }
}
