{
  "name": "code-space",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "crates/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clear": "turbo run clear",
    "tsnode": "ts-node-esm scripts/sw.ts",
    "update": "turbo run update --parallel",
    "format": "prettier --loglevel=warn --write \"**/*.{mjs,ts,tsx,scss,md}\"",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "type": "module",
  "dependencies": {
    "node-fetch": "^3.2.10",
    "whatwg-fetch": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.1",
    "@types/express": "^4.17.14",
    "@types/node": "^16.11.59",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.31.8",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^6.7.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.16",
    "ts-node": "^10.9.1",
    "turbo": "latest",
    "typescript": "^4.8.3",
    "wasm-pack": "^0.10.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "vite": "^3.1.1",
    "vitest": "^0.23"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.5.1"
}
